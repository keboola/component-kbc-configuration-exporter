{
  "type": "object",
  "title": "configuration",
  "required": [
    "#api_token",
    "#src_token",
    "aws_region",
    "dst_aws_region"
  ],
  "properties": {
    "#api_token": {
      "type": "string",
      "title": "Manage token",
      "format": "password",
      "propertyOrder": 200
    },
    "#src_token": {
      "type": "string",
      "title": "Source project token",
      "format": "password",
      "propertyOrder": 300
    },
    "aws_region": {
      "type": "string",
      "title": "Source Region",
      "enum": [
        "US",
        "EU",
        "AZURE-EU",
        "GCP-US",
        "GCP-EU",
        "CUSTOM-SRC"
      ],
      "options": {
        "enum_titles": [
          "US - .keboola.com",
          "EU - .eu-central-1.keboola.com",
          "AZURE-EU - .north-europe.azure.keboola.com",
          "GCP-US - .us-east4.gcp.keboola.com",
          "GCP-EU - .europe-west3.gcp.keboola.com",
          "CUSTOM"
        ]
      },
      "propertyOrder": 500,
      "default": "US"
    },
    "custom_region": {
      "type": "string",
      "title": "Custom Source Region",
      "default": "",
      "description": "Custom source region name in the format: .name.keboola.cloud",
      "options": {
        "dependencies": {
          "aws_region": "CUSTOM-SRC"
        }
      },
      "propertyOrder": 550
    },
    "dst_aws_region": {
      "type": "string",
      "title": "Destination Region",
      "propertyOrder": 600,
      "enum": [
        "US",
        "EU",
        "AZURE-EU",
        "GCP-US",
        "GCP-EU",
        "CUSTOM-DST"
      ],
      "options": {
        "enum_titles": [
          "US - .keboola.com",
          "EU - .eu-central-1.keboola.com",
          "AZURE-EU - .north-europe.azure.keboola.com",
          "GCP-US - .us-east4.gcp.keboola.com",
          "GCP-EU - .europe-west3.gcp.keboola.com",
          "CUSTOM"
        ]
      },
      "default": "US"
    },
    "custom_dst_region": {
      "type": "string",
      "title": "Custom Destination Region",
      "default": "",
      "description": "Custom destination region in the format: .name.keboola.cloud",
      "options": {
        "dependencies": {
          "dst_aws_region": "CUSTOM-DST"
        }
      },
      "propertyOrder": 650
    }
  }
}